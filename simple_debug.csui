repo_name=$(g_repo)
today=$(t_date)
user_id=$(n_user)
count_f="/tmp/comsui_${repo_name}_${today}_${user_id}"

info "repo_name: $repo_name"
info "today: $today"
info "user_id: $user_id"
info "count_f: $count_f"

if f_no "$count_f"; then
    info "File does not exist, creating it"
    echo "0" > "$count_f"
else
    info "File exists"
fi

count=$(f_read "$count_f" "DEFAULT")
info "Read count: $count"